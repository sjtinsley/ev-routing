    "koa-bodyparser": "^4.3.0",
    "koa-router": "^11.0.1",
    "koa-static": "^5.0.0",
    "mapbox-gl": "^2.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.0.17",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
